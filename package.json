{
  "name": "jimbos-nfl-pools",
  "version": "1.0.0",
  "description": "Track current (and previous) week spread pool results.",
  "scripts": {
    "clean": "rimraf _site",
    "test": "jest",
    "dev": "run-p dev:*",
    "dev:11ty": "eleventy --serve --watch",
    "dev:css": "tailwindcss -i ./css/styles.css -o ./_site/styles.css --watch",
    "build": "NODE_ENV=production npm-run-all clean build:11ty build:css",
    "build:11ty": "eleventy --pathprefix=jimbos-nfl-pool",
    "build:css": "tailwindcss -i ./css/styles.css -o ./_site/styles.css --minify",
    "prettier": "prettier --write -c '{content,css,src}/**/*.{js,json,css,njk}'",
    "prettier:check": "prettier --check -c '{content,css,src}//**/*.{js,json,css,njk}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.16",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "eslint-config-prettier": "^8.5.0",
    "html-minifier": "^4.0.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.2.3",
    "xlsx": "^0.18.5"
  },
  "11ty": {
    "title": "Jimbo's NFL Pools"
  },
  "dependencies": {
    "@11ty/eleventy-fetch": "^3.0.0"
  }
}
